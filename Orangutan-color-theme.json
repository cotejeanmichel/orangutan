  "tokenColors": [
    // foreground
    {
      "name": "#555", "settings": { "foreground": "#555" },
      "scope": [
        "comment",
        "punctuation.definition.tag",
        "punctuation.section.property-list.begin.bracket",
        "punctuation.section.property-list.end.bracket",
        "punctuation.definition.dictionary.begin.json.comments",
        "punctuation.definition.dictionary.end.json.comments",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx",
        "punctuation.definition.interpolation",
        "punctuation.definition.block",
        "punctuation.definition.template-expression",
        "punctuation.definition.dictionary.begin",
        "punctuation.definition.dictionary.end",
        "punctuation.definition.binding-pattern.object",
        "punctuation.accessor",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.parameters",
        // .css, .scss
        "punctuation.section.function",
        "punctuation.definition.media-query.begin.bracket",
        "punctuation.definition.media-query.end.bracket",
        "punctuation.definition.attribute-selector.begin.bracket",
        "punctuation.definition.attribute-selector.end.bracket",
        // .md
        "punctuation.definition.heading.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.constant.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.markdown",
        "meta.separator.markdown",
      ],
    },
    {
      "name": "#888", "settings": { "foreground": "#888" },
      "scope": [
        "keyword",
        "storage",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.entity",
        // .md
        "punctuation.definition.list.begin.markdown",
        "variable.object.property",
        "string.quoted",
        "string.template",
        "entity.other.attribute-name.html"
      ]
    },
    {
      "name": "#aaa", "settings": { "foreground": "#aaa", "fontStyle": "italic" },
      "scope": [
        "meta.object-literal.key",
        "string.quoted.double"
      ]
    },

    {
      "name": "#ee8f28", "settings": { "foreground": "#ee8e28d5" },
      "scope": [
        "entity.name",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "support.function",
        // .css, .scss
        "keyword.other.unit",
      ]
    },
    {
      "name": "bold", "settings": {"fontStyle": "bold"},
      "scope": [
        "meta.definition.method",
        "support.class",
        "support.function",
        // "support.class.builtin",
        // "support.class.promise",
        "support.type.primitive",
        "storage.type.function",
        "storage.type.class",
        "storage.type.interface",
        "support.class.component",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        // .md
        "markup.heading.markdown",
        "markup.bold.markdown",
        "entity.name.type",
      ]
    },
    {
      "name": "keywords", "settings": {"foreground": "#ffffff"},
      "scope": [
        "variable.language.this",
        "string.regexp",
        "keyword.control",
        "storage.modifier.async",
        "keyword.operator.new",
        "support.type.object.module",
        "constant.language.undefined",
        "constant.language.null",
        "storage.modifier",
        "keyword.operator.expression.typeof",
        // html
        "string.unquoted.html",
        "text.html.derivative"
      ]
    },
    {
      "name": "#f2b16b", "settings": { "foreground": "#f2b16bd7" },
      "scope": [
        "support.type.property-name",
        "constant.language.boolean",
        "support.type.primitive",
        // .css, .scss
        "variable.scss",
        "variable.scss",
        "support.constant.property-value",
        "support.function.misc",
        "meta.property-value.media-query",
        "constant.other.color.rgb-value",
        "meta.link.reference.markdown",
        "meta.template.expression",
        "markup.inline.raw.string.markdown"
      ]
    },
    {
      "name": "#f2b16b", "settings": { "foreground": "#ffae00" },
      "scope": [
        "constant.numeric",
        "constant.language.json"
      ]
    },

    // fontStyle
    {
      "name": "italic", "settings": { "fontStyle": "italic" },
      "scope": [
        // "keyword.control",
        "storage",
        "comment",
        // "constant.language.boolean",
        "constant.language.json.comments",
        "keyword.operator.optional",
        "support.type.primitive",
        // .md
        "markup.italic.markdown",
        "markup.quote.markdown",
        // "variable.other.readwrite",
        "variable.parameter",
        // "meta.template.expression",
        "markup.inline.raw.string.markdown"
      ]
    },
    {
      "name": "normal", "settings": { "fontStyle": "" },
      "scope": [
        "storage.type.function.arrow",
        // .md
        "markup.underline.link.markdown",
      ]
    }
  ]
}

